generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model autochannels {
  id            Int    @id @default(autoincrement())
  guild         String @db.VarChar
  origenchannel String @db.VarChar(30)
  targetchannel String @db.VarChar(30)
}

model guilds {
  id       Int    @id @default(autoincrement())
  guild    String @db.VarChar @unique
  welcome  String @db.VarChar(30)
  birthday String @db.VarChar(30)
  stalk    Int
  bdaymsg  String @db.VarChar(600)
  bdayutc  Int
  type     Int
  prefix   String @db.VarChar(10)
  levels   Int
}

model levelchannels {
  id      Int    @id @default(autoincrement())
  guild   String @db.VarChar
  channel String @db.VarChar(30)
}

model levels {
  id     Int    @id @default(autoincrement())
  guild  String @db.VarChar
  uid    String @db.VarChar
  points Int
  xp     Int
  cd     Int
  level  Int
}

model logs {
  id          Int    @id @default(autoincrement())
  guild       String @db.VarChar
  time        Int
  description String @db.VarChar
}

model msgcustoms {
  id      Int    @id @default(autoincrement())
  time    Int
  guild   String @db.VarChar
  name    String @db.VarChar(50)
  message String @db.VarChar(500)
}

model oraculos {
  id    Int    @id @default(autoincrement())
  guild String @db.VarChar
  msg   String @db.VarChar(500)
}

model purges {
  id      Int    @id @default(autoincrement())
  guild   String @db.VarChar
  channel String @db.VarChar(30)
  hour    Int
  minute  Int
  utc     Int
}

model shops {
  id      Int    @id @default(autoincrement())
  guild   String @db.VarChar
  name    String @db.VarChar(120)
  role    String @db.VarChar(30)
  channel String @db.VarChar(30)
  price   Int
  type    Int
}

model socials {
  id          Int    @id @default(autoincrement())
  guild       String @db.VarChar
  channel     String @db.VarChar(30)
  name        String @db.VarChar(50)
  last_update String @db.VarChar
  live        Int
  type        Int
  real_name   String @db.VarChar(50)
}

model stalkmsgs {
  id    Int    @id @default(autoincrement())
  guild String @db.VarChar
  msg   String @db.VarChar(250)
}

model stalkroles {
  id    Int    @id @default(autoincrement())
  guild String @db.VarChar
  role  String @db.VarChar(35)
}

model users {
  id    Int    @id @default(autoincrement())
  uid   String @db.VarChar
  month Int
  day   Int
}

model welcomes {
  id    Int    @id @default(autoincrement())
  guild String @db.VarChar
  type  Int
  msg   String @db.VarChar(200)
}